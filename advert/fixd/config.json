{
    "name": "定位",
    "plugin": "adsFixed",
    "selector": "#adsFixed",
    "isPercent": "enable",
    "count": 1,
    "type": "attach",
    "note": "本脚本需要 jQuery 支持",
    "opts": [
        {
            "field": "loading",
            "label": "载入消息",
            "value": "正在载入广告 ..."
        },
        {
            "field": "position",
            "label": "定位（可选 left-top、right-top、left-bottom、right-bottom）",
            "value": "left-top"
        },
        {
            "field": "top",
            "label": "上边距（定位为 left-top、right-top 时有效）",
            "value": "50px"
        },
        {
            "field": "bottom",
            "label": "下边距（定位为 left-bottom、right-bottom 时有效）",
            "value": "50px"
        },
        {
            "field": "left",
            "label": "左边距（定位为 left-top、left-bottom 时有效）",
            "value": "10px"
        },
        {
            "field": "right",
            "label": "右边距（定位为 right-top、right-bottom 时有效）",
            "value": "10px"
        },
        {
            "field": "close",
            "label": "关闭文本",
            "value": "&times; 关闭"
        }
    ]
}